{
  "name": "sockskit",
  "version": "0.0.2",
  "license": "MIT",
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "bin": {
    "sockskit": "./index.js"
  },
  "scripts": {
    "start": "ts-node src/socks.ts",
    "dev": "nodemon --exec ts-node src/socks.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsup && cp README.md ./dist/ && cp package.json ./dist/ && cp LICENSE ./dist/"
  },
  "devDependencies": {
    "@aws-sdk/s3-presigned-post": "^3.438.0",
    "commander": "^11.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "esbuild": "^0.19.5"
  }
}
