{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 18",
  "_version": "18.2.0",
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM"
    ],
    "module": "ESNext",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Bundler",
    "outDir": "./dist",
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "baseUrl": "../..",
    "paths": {
      "@websocks/client": [
        "packages/client/src"
      ],
      "@websocks/client/*": [
        "packages/client/src/*"
      ],
      "@websocks/server": [
        "packages/server/src"
      ],
      "@websocks/server/*": [
        "packages/server/src/*"
      ]
    },
    "emitDeclarationOnly": false,
    "incremental": true,
    "tsBuildInfoFile": "C:/Users/rs143/Desktop/s/websocks/node_modules/vitest/dist/tsconfig.tmp.tsbuildinfo"
  },
  "exclude": [
    "dist",
    "./dist"
  ],
  "include": [
    "src"
  ]
}